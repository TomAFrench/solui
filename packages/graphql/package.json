{
  "name": "@solui/graphql",
  "version": "0.1.1",
  "description": "GraphQL utilities for solUI",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf ./dist && mkdir ./dist",
    "build": "yarn clean && babel ./src --out-dir ./dist",
    "watch": "yarn build --watch"
  },
  "keywords": [
    "graphql",
    "client",
    "solui",
    "ethereum",
    "solidity",
    "ui"
  ],
  "files": [
    "dist"
  ],
  "author": "Ramesh Nair <ram@hiddentao.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solui/solui.git"
  },
  "bugs": {
    "url": "https://github.com/solui/solui/issues"
  },
  "homepage": "https://solui.dev",
  "devDependencies": {
    "@babel/preset-env": "^7.6.0",
    "core-js": "^3.2.1"
  },
  "dependencies": {
    "@solui/utils": "^0.1.1",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.2",
    "graphql": "^14.5.8",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "graphql-type-json": "^0.3.1",
    "node-fetch": "^2.6.0"
  }
}
