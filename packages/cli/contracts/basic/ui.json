{
  "version": 1,
  "id": "solui-repo-basic",
  "title": "Basic contract test",
  "description": "Lorem ipsum dolor sit amet, consectetur adipiscing elit. Cras placerat metus a laoreet luctus. Donec ut imperdiet ligula. Nullam vel nisi fringilla nulla mollis congue at sed dui. Nullam congue ipsum at tellus aliquet, eget tincidunt orci efficitur. Suspendisse vel efficitur ante. Praesent commodo nisi faucibus cursus aliquam. Fusce ultrices.",
  "image": {
    "url": "https://upload.wikimedia.org/wikipedia/commons/a/af/Twemoji_26c4.svg"
  },
  "groups": [
    {
      "id": "group1",
      "title": "Deployment",
      "description": "Deploy new instances of the contract. Lorem ipsum dolor sit amet, consectetur adipiscing elit. Cras placerat metus a laoreet luctus. Donec ut imperdiet ligula. Nullam vel nisi fringilla nulla mollis congue at sed dui. Nullam congue ipsum at tellus aliquet, eget tincidunt orci efficitur. Suspendisse vel efficitur ante. Praesent commodo nisi faucibus cursus aliquam. Fusce ultrices.",
      "image": {
        "url": "https://upload.wikimedia.org/wikipedia/commons/c/c6/Builder_by_mimooh.svg"
      },
      "panels": [
        {
          "id": "deployInstance",
          "title": "Deploy new instance",
          "inputs": [
            {
              "name": "owner",
              "title": "Owner",
              "type": "address",
              "initialValue": "0x0000000000000000000000000000000000000000",
              "addressType": [ "eoa" ]
            }
          ],
          "execs": [
            {
              "type": "deploy",
              "contract": "Ownable",
              "args": {
                "_owner": "owner"
              },
              "saveResultAs": "contractAddress"
            }
          ],
          "outputs": [
            {
              "title": "New contract address",
              "type": "address",
              "param": "contractAddress"
            }
          ]
        }
      ]
    },
    {
      "id": "group2",
      "title": "Interaction ops",
      "description": "Interact with contract instances",
      "image": {
        "url": "https://upload.wikimedia.org/wikipedia/commons/6/6e/Breezeicons-actions-22-im-user.svg"
      },
      "inputs": [
        {
          "name": "contractAddress",
          "title": "Contract address",
          "type": "address",
          "addressType": [ "contract" ]
        }
      ],
      "panels": [
        {
          "id": "showOwner",
          "title": "Show owner",
          "execs": [
            {
              "type": "call",
              "contract": "Ownable",
              "address": "contractAddress",
              "method": "owner",
              "saveResultAs": "ownerAddress"
            }
          ],
          "outputs": [
            {
              "title": "Current owner",
              "type": "address",
              "param": "ownerAddress"
            }
          ]
        },
        {
          "id": "changeOwner",
          "title": "Change owner",
          "inputs": [
            {
              "name": "newOwner",
              "title": "New owner",
              "type": "address",
              "addressType": [ "eoa" ]
            }
          ],
          "execs": [
            {
              "type": "send",
              "contract": "Ownable",
              "address": "contractAddress",
              "method": "transferOwnership",
              "args": {
                "_newOwner": "newOwner"
              }
            }
          ]
        }
      ]
    }
  ]
}
