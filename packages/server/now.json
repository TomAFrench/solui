{
  "version": 2,
  "name": "solui-live",
  "alias": "solui.dev",
  "regions": [ "bru" ],
  "builds": [{ "src": "package.json", "use": "@now/next" }],
  "env": {
    "NODE_ENV": "production",
    "APP_MODE": "live",
    "ENCRYPTION_IV": "@encryption_iv",
    "ENCRYPTION_KEY": "@encryption_key",
    "MAILGUN_API_KEY": "@mailgun_api_key",
    "DB_HOST": "@db_host",
    "DB_PORT": "@db_port",
    "DB_USERNAME": "@db_username",
    "DB_PASSWORD": "@db_password"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "APP_MODE": "live",
      "ENCRYPTION_IV": "@encryption_iv",
      "ENCRYPTION_KEY": "@encryption_key",
      "MAILGUN_API_KEY": "@mailgun_api_key",
      "DB_HOST": "@db_host",
      "DB_PORT": "@db_port",
      "DB_USERNAME": "@db_username",
      "DB_PASSWORD": "@db_password"
    }
  }
}
