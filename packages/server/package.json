    "@solui/styles": "^0.0.0",
    "@solui/utils": "^0.0.0",
    "@zeit/next-css": "^1.0.1",
    "apollo-cache-inmemory": "^1.3.12",
    "apollo-client": "^2.4.8",
    "apollo-link": "^1.2.6",
    "apollo-link-http": "^1.5.9",
    "apollo-server-koa": "^2.3.1",
    "bunyan": "^1.8.12",
    "bunyan-format": "^0.2.1",
    "color": "^3.1.1",
    "cryptocurrency-icons": "^0.9.3",
    "date-fns": "^2.0.0-alpha.27",
    "emotion-theming": "^10.0.19",
