{
  "name": "@solui/server",
  "private": true,
  "version": "1.0.0",
  "description": "SolUI API server and website",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "lint": "eslint .",
    "start": "nodemon index.js",
    "db:migrate": "yarn knex migrate:latest",
    "db:migrate:live": "yarn knex --env live migrate:latest",
    "db:rollback": "yarn knex migrate:rollback",
    "db:seed": "yarn knex seed:run",
    "knex": "babel-node --presets @babel/env node_modules/.bin/knex",
    "now": "now --token $NOW_TOKEN --scope solui",
    "deploy": "yarn now --prod -f --public",
    "clean-old-deploys": "yarn now rm solui-live --safe --yes"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
