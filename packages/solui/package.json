{
  "name": "solui",
  "descrption": "Declarative UIs for smart contracts.",
  "version": "0.1.0",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@emotion/core": "^10.0.16",
    "@emotion/styled": "^10.0.15",
    "@koa/cors": "^2.2.3",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "emotion-theming": "^10.0.14",
    "glob": "^7.1.4",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "lodash.foreach": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "web3": "^1.2.1",
    "web3-utils": "^1.2.1",
    "yargs": "^14.0.0"
  },
  "scripts": {
    "lint": "eslint . && solhint contracts/**/*.sol",
    "build": "next build",
    "start": "nodemon bin/solui --artifacts ./build/contracts --ui ./contracts/ui.json",
    "truffle": "babel-node node_modules/.bin/truffle",
    "tc:devnet": "ganache-cli -a 10",
    "tc:devnet-ci": "ganache-cli -a 10 >ganache.log 2>&1 &",
    "tc:clean": "yarn truffle networks --clean",
    "tc:deploy": "yarn truffle compile && yarn truffle migrate"
  },
  "devDependencies": {
    "@babel/node": "^7.5.5",
    "babel-eslint": "9.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.18.11",
    "react-scripts": "2.1.8",
    "solhint": "^2.2.0",
    "truffle": "^5.0.33"
  }
}
