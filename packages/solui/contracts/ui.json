{
  "version": 1,
  "id": "solui-repo-ownable",
  "description": "The ownable!",
  "groups": [
    {
      "id": "group1",
      "title": "Deployment ops...",
      "panels": [
        {
          "id": "deployInstance",
          "title": "Deploy new instance",
          "inputs": {
            "owner": {
              "title": "Owner",
              "type": "address",
              "initialValue": "0x0000000000000000000000000000000000000000",
              "addressType": [ "eoa" ]
            }
          },
          "execs": [
            {
              "type": "deploy",
              "contract": "Ownable",
              "args": {
                "_owner": "owner"
              },
              "saveResultAs": "contractAddress"
            }
          ],
          "output": {
            "title": "New contract address",
            "type": "address",
            "param": "contractAddress"
          }
        }
      ]
    },
    {
      "id": "group2",
      "title": "Interaction ops...",
      "inputs": {
        "contract1": {
          "title": "Contract address",
          "type": "address",
          "addressType": [ "contract" ]
        }
      },
      "panels": [
        {
          "id": "showOwner",
          "title": "Show owner",
          "execs": [
            {
              "type": "call",
              "contract": "Ownable",
              "address": "contract1",
              "method": "owner",
              "saveResultAs": "ownerAddress"
            }
          ],
          "output": {
            "title": "Current owner",
            "type": "address",
            "param": "ownerAddress"
          }
        },
        {
          "id": "changeOwner",
          "title": "Change owner",
          "inputs": {
            "newOwner": {
              "title": "New owner",
              "type": "address",
              "addressType": [ "eoa" ]
            }
          },
          "execs": [
            {
              "type": "send",
              "contract": "Ownable",
              "address": "contract1",
              "method": "transferOwnership",
              "args": {
                "_newOwner": "newOwner"
              }
            }
          ]
        }
      ]
    }
  ]
}
