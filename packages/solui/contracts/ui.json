{
  "deployInstance": {
    "title": "Deploy new instance",
    "inputs": {
      "owner": {
        "title": "Owner",
        "type": "address",
        "initialValue": "0x0000000000000000000000000000000000000000",
        "addressType": [
          "eoa"
        ]
      }
    },
    "execs": [
      {
        "type": "deploy",
        "contract": "Ownable",
        "params": {
          "_owner": "owner"
        },
        "saveResultAs": "contractAddress"
      }
    ],
    "output": {
      "title": "New contract address",
      "type": "address",
      "param": "contractAddress"
    }
  },
  "showOwner": {
    "title": "Show owner",
    "execs": [
      {
        "type": "call",
        "contract": "Ownable",
        "method": "owner"
      }
    ]
  },
  "changeOwner": {
    "title": "Change owner",
    "inputs": {
      "newOwner": {
        "title": "New owner",
        "type": "address",
        "valueNotEq": "{msg.sender}",
        "addressType": [
          "eoa"
        ]
      }
    },
    "execs": [
      {
        "type": "transaction",
        "contract": "Ownable",
        "method": "transferOwnership",
        "params": {
          "_newOwner": "newOwner"
        }
      }
    ]
  }
}
